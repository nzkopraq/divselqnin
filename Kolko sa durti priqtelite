#include<iostream>
using namespace std;
int main()
{
	int i,b=0,c,a;
	cout<<"kolko hora poznavash?"<<endl;
	cin>>a;
	int age[100];
	for(i=0;i<=a-1;i++)
	{
		cout<<"Kolko e durt "<<i+1<<"-iq?"<<endl;
		cin>>age[i];
	}
	c=age[0];
	for(i=0;i<=a-1;i++)
	{
		if(b<=age[i]) b=age[i];
		if(c>=age[i]) c=age[i];
	}
	cout<<"Nai blizko do smurtta e "<<b<<"-godi6niqt"<<endl;
	cout<<"Tozi deto posleden e sprql da nosi pampersi e "<<c<<"-godi6en"<<endl;
	system("pause");
	return 0;

}
