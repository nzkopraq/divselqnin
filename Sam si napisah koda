#include<iostream>
#include<string>
using namespace std;
bool zakonmiedumata(int a)
{
	if(a%2==0) return true;
	return false;
}
int main()
{
	int a;
	cout<<"Kaji nqkvo 4islo."<<endl;
	cin>>a;
	string isEven = zakonmiedumata(a) ? "true" : "false"; \\tova opredeleno go izmislih az
	cout << isEven <<endl;
	system("pause");
	return 0;
}
