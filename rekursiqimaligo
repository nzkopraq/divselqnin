#include<iostream>
#include<string>
using namespace std;
 bool n(int a,int b)
 {
	 if(a/10<1) return 0;
	 else if(a%10==b) return 1;
	 return n(a/10,b);
 }
 int main()
 {
	 int c,d;
	 cout<<"Ot koeto tursim=";
	 cin>>c;
	 cout<<"Koeto tursim=";
	 cin>>d;
	 string pruc = n(c,d) ? "ima go" : "nego go nema vuv celata shema" ;
	 cout<< pruc<<endl;
	 system ("pause");
	 return 0;
 }
